{
  "id": "ninebot.auto.sign",
  "name": "九号自动签到（单账号）",
  "version": "1.2",
  "author": "QinyRui",
  "apps": [
    {
      "id": "Ninebot_Auto_Sign",
      "name": "九号签到（单账号）",
      "keys": [
        "ninebot.authorization",
        "ninebot.deviceId",
        "ninebot.userAgent",
        "ninebot.debug",
        "ninebot.notify",
        "ninebot.autoOpenBox",
        "ninebot.titlePrefix"
      ],
      "settings": [
        { "id": "ninebot.authorization", "name": "Authorization", "val": "", "type": "text" },
        { "id": "ninebot.deviceId", "name": "DeviceId", "val": "", "type": "text" },
        { "id": "ninebot.userAgent", "name": "User-Agent", "val": "", "type": "text" },
        { "id": "ninebot.debug", "name": "调试日志开关", "type": "boolean", "val": true },
        { "id": "ninebot.notify", "name": "通知开关", "type": "boolean", "val": true },
        { "id": "ninebot.autoOpenBox", "name": "自动开启盲盒", "type": "boolean", "val": true },
        { "id": "ninebot.titlePrefix", "name": "自定义标题", "val": "九号签到", "type": "text" }
      ]
    }
  ]
}