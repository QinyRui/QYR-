#!version = 1.0
#!system_version = 15

[Argument]
debugLevel = select, "0", "1", "2", "3", "4", "5", "6", "7", tag=日志等级
progressStyle = select, "0", "1", "2", "3", "4", "5", "6", "7", tag=盲盒进度条样式
titlePrefix = input, "九号签到", tag=通知标题

[Script]
# 定时执行脚本，并传入参数
cron 0 11 * * * script-path=https://raw.githubusercontent.com/yourrepo/Ninebot_Sign_ArgDemo.js,timeout=60,tag=九号-自动签到,argument=[{debugLevel},{progressStyle},{titlePrefix}]