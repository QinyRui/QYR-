{
  "id": "Ninebot.Sign.Single",
  "name": "九号智能电动车 · 单号自动签到",
  "version": "2.7.0",
  "author": "适配专用",
  "description": "兼容 Ninebot_Sign_Single_v2.7.js，可视化管理核心功能",
  "keys": [
    {
      "key": "ninebot.authorization",
      "type": "input",
      "title": "Authorization",
      "desc": "抓包自动写入，无需手动填",
      "default": ""
    },
    {
      "key": "ninebot.deviceId",
      "type": "input",
      "title": "DeviceId",
      "desc": "抓包自动写入",
      "default": ""
    },
    {
      "key": "ninebot.userAgent",
      "type": "input",
      "title": "User-Agent",
      "default": "Ninebot/3620 CFNetwork/3860.200.71 Darwin/25.1.0"
    },
    {
      "key": "ninebot.shareTaskUrl",
      "type": "input",
      "title": "分享接口",
      "desc": "抓包分享动作获取",
      "default": "https://snssdk.ninebot.com/service/2/app_log/?aid=10000004"
    },
    {
      "key": "ninebot.logLevel",
      "type": "selector",
      "title": "日志等级",
      "options": [
        {"label": "调试", "value": "debug"},
        {"label": "信息", "value": "info"},
        {"label": "警告", "value": "warn"},
        {"label": "错误", "value": "error"}
      ],
      "default": "info"
    },
    {
      "key": "ninebot.notify",
      "type": "switch",
      "title": "启用通知",
      "default": true
    },
    {
      "key": "ninebot.notifyFail",
      "type": "switch",
      "title": "失败通知",
      "default": true
    },
    {
      "key": "ninebot.autoOpenBox",
      "type": "switch",
      "title": "自动开盲盒",
      "default": false
    },
    {
      "key": "ninebot.titlePrefix",
      "type": "input",
      "title": "通知前缀",
      "default": "九号签到助手"
    },
    {
      "key": "ninebot.lastCaptureAt",
      "type": "text",
      "title": "最后抓包时间",
      "editable": false,
      "default": ""
    },
    {
      "key": "ninebot.lastShareDate",
      "type": "text",
      "title": "最后分享日期",
      "editable": false,
      "default": ""
    }
  ]
}