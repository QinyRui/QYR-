{
  "id": "Ninebot.Sign.Single",
  "name": "九号智能电动车 · 单号自动签到",
  "author": "QinyRui",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/logo_128.png",
  "repo": "https://t.me/JiuHaoAPP",
  "apps": [
    {
      "id": "ninebot.single",
      "name": "九号智能电动车（单号）",
      "keys": [
        "ninebot.authorization",
        "ninebot.deviceId",
        "ninebot.userAgent",
        "ninebot.titlePrefix",
        "ninebot.notify",
        "ninebot.autoOpenBox",
        "ninebot.notifyFail",
        "ninebot.logLevel",
        "ninebot.lastCaptureAt"
      ],
      "settings": [
        {
          "id": "ninebot.authorization",
          "name": "Authorization",
          "val": "",
          "type": "text",
          "desc": "账号鉴权Token（抓包自动写入，无需手填）"
        },
        {
          "id": "ninebot.deviceId",
          "name": "DeviceId",
          "val": "",
          "type": "text",
          "desc": "设备ID（抓包自动写入，无需手填）"
        },
        {
          "id": "ninebot.userAgent",
          "name": "User-Agent",
          "val": "Ninebot/3620 CFNetwork/3860.200.71 Darwin/25.1.0",
          "type": "text",
          "desc": "请求UA（抓包自动覆盖，默认值可直接使用）"
        },
        {
          "id": "ninebot.titlePrefix",
          "name": "自定义通知标题",
          "val": "九号签到助手",
          "type": "text",
          "desc": "通知标题前缀，例如：九号签到助手"
        },
        {
          "id": "ninebot.notify",
          "name": "开启签到通知",
          "val": "true",
          "type": "select",
          "items": [
            { "key": "true", "label": "开启（推送签到结果）" },
            { "key": "false", "label": "关闭（不推送通知）" }
          ],
          "desc": "是否推送签到结果通知"
        },
        {
          "id": "ninebot.autoOpenBox",
          "name": "自动开启盲盒",
          "val": "false",
          "type": "select",
          "items": [
            { "key": "true", "label": "开启（所有可用盲盒自动开箱）" },
            { "key": "false", "label": "关闭（手动开箱）" }
          ],
          "desc": "支持7天盲盒+普通盲盒，满足条件自动开启"
        },
        {
          "id": "ninebot.notifyFail",
          "name": "失败通知开关",
          "val": "true",
          "type": "select",
          "items": [
            { "key": "true", "label": "开启（失败时推送通知）" },
            { "key": "false", "label": "关闭（失败时不推送）" }
          ],
          "desc": "仅在失败时推送通知，避免打扰"
        },
        {
          "id": "ninebot.logLevel",
          "name": "日志输出级别",
          "val": "debug",
          "type": "select",
          "items": [
            { "key": "silent", "label": "静默（无日志）" },
            { "key": "simple", "label": "简洁（仅错误日志）" },
            { "key": "full", "label": "完整（所有日志）" }
          ],
          "desc": "细化日志控制"
        },
        {
          "id": "ninebot.lastCaptureAt",
          "name": "最后抓包时间",
          "val": "",
          "type": "text",
          "desc": "所有配置最后抓包更新时间（仅展示，不可修改）"
        }
      ]
    }
  ]
}