{
  "name": "Ninebot Sign v3.1 Share+ 完整版",
  "description": "九号智能电动车自动签到 + 自动抓 Token + 盲盒 + 多账号 + N币余额",
  "scripts": [
    {
      "name": "Ninebot Token Capture",
      "type": "http-request",
      "pattern": "^https:\\/\\/cn-cbu-gateway\\.ninebot\\.com\\/portal\\/api\\/user-sign",
      "script": "https://raw.githubusercontent.com/QinyRui/QYR-/main/Ninebot_Capture.js",
      "enable": true
    },
    {
      "name": "Ninebot Auto Sign",
      "type": "cron",
      "cron": "0 8 * * *",
      "script": "https://raw.githubusercontent.com/QinyRui/QYR-/main/Ninebot_Sign_Share_v3.1.js",
      "timeout": 60,
      "enable": true
    }
  ],
  "variables": [
    {
      "key": "NINEBOT_ACCOUNTS",
      "name": "九号账号列表",
      "desc": "多账号 JSON 列表，支持自定义昵称、Authorization、DeviceId",
      "val": "[]"
    }
  ],
  "boxjs": {
    "id": "ninebot.app",
    "name": "九号电动车签到",
    "keys": ["NINEBOT_ACCOUNTS"],
    "settings": [
      {
        "id": "NINEBOT_ACCOUNTS",
        "name": "账号列表（JSON）",
        "val": "[]",
        "type": "textarea",
        "placeholder": "[{\"name\":\"主号\",\"authorization\":\"xxx\",\"deviceId\":\"xxx\"}]"
      }
    ]
  }
}