{
  "id": "ninebot",
  "name": "九号电动车双账号签到配置",
  "author": "QinyRui（双账号适配版）",
  "desc": "支持双账号独立签到、开箱、补签、通知配置，含账号启停开关",
  "settings": [
    {
      "id": "ninebot.account1.enable",
      "name": "账号1 启用开关",
      "val": "true",
      "type": "select",
      "items": [
        { "key": "true", "label": "开启（执行签到/开箱）" },
        { "key": "false", "label": "关闭（跳过该账号）" }
      ],
      "desc": "关闭后脚本将忽略账号1的所有任务"
    },
    {
      "id": "ninebot.account1.authorization",
      "name": "账号1 Authorization",
      "val": "",
      "type": "text",
      "desc": "抓包获取的账号1授权令牌，必填"
    },
    {
      "id": "ninebot.account1.deviceId",
      "name": "账号1 DeviceId",
      "val": "",
      "type": "text",
      "desc": "抓包获取的账号1设备ID，必填"
    },
    {
      "id": "ninebot.account1.userAgent",
      "name": "账号1 User-Agent",
      "val": "Mozilla/5.0 (iPhone; CPU iPhone OS 18_7 like Mac OS X) AppleWebKit/605.1.15 (KHTML, like Gecko) Mobile/15E148 Segway v6 C 609113620",
      "type": "text",
      "desc": "抓包获取的UA，保持默认即可"
    },
    {
      "id": "ninebot.account1.titlePrefix",
      "name": "账号1 通知标题前缀",
      "val": "九号签到助手（账号1）",
      "type": "text",
      "desc": "自定义账号1的通知标题开头"
    },
    {
      "id": "ninebot.account1.notify",
      "name": "账号1 通知开关",
      "val": "true",
      "type": "select",
      "items": [
        { "key": "true", "label": "开启通知" },
        { "key": "false", "label": "关闭通知" }
      ],
      "desc": "是否发送账号1的签到结果通知"
    },
    {
      "id": "ninebot.account1.notifyFail",
      "name": "账号1 异常通知开关",
      "val": "true",
      "type": "select",
      "items": [
        { "key": "true", "label": "开启异常通知" },
        { "key": "false", "label": "关闭异常通知" }
      ],
      "desc": "仅在脚本执行出错时发送通知"
    },
    {
      "id": "ninebot.account1.autoOpenBox",
      "name": "账号1 自动开箱开关",
      "val": "true",
      "type": "select",
      "items": [
        { "key": "true", "label": "自动开启可领取盲盒" },
        { "key": "false", "label": "关闭自动开箱" }
      ],
      "desc": "自动检测并开启所有可领取的盲盒"
    },
    {
      "id": "ninebot.account1.autoRepairCard",
      "name": "账号1 自动补签开关",
      "val": "true",
      "type": "select",
      "items": [
        { "key": "true", "label": "签到失败自动补签" },
        { "key": "false", "label": "关闭自动补签" }
      ],
      "desc": "需补签卡，签到失败时自动使用补签卡"
    },
    {
      "id": "ninebot.account1.enableRetry",
      "name": "账号1 请求重试开关",
      "val": "true",
      "type": "select",
      "items": [
        { "key": "true", "label": "开启请求重试" },
        { "key": "false", "label": "关闭请求重试" }
      ],
      "desc": "网络波动时自动重试请求，提高成功率"
    },
    {
      "id": "ninebot.account1.logLevel",
      "name": "账号1 日志等级",
      "val": "full",
      "type": "select",
      "items": [
        { "key": "silent", "label": "静默（无日志）" },
        { "key": "simple", "label": "简单（仅警告/错误）" },
        { "key": "full", "label": "完整（所有日志）" }
      ],
      "desc": "控制脚本日志输出详细程度"
    },
    {
      "id": "ninebot.account1.lastCaptureAt",
      "name": "账号1 最后抓包时间",
      "val": "",
      "type": "text",
      "desc": "自动生成，无需手动修改"
    },
    {
