{
  "id": "Ninebot.Sign.Single",
  "name": "九号智能电动车 · 单号自动签到（BoxJS）",
  "author": "QinyRui",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/logo_128.png",
  "repo": "https://t.me/JiuHaoAPP",
  "apps": [
    {
      "id": "ninebot.single",
      "name": "九号智能电动车（单号）",
      "keys": [
        "ninebot.authorization",
        "ninebot.deviceId",
        "ninebot.userAgent",
        "ninebot.shareTaskUrl",
        "ninebot.progressStyle",
        "ninebot.titlePrefix",
        "ninebot.logLevel"
      ],
      "settings": [
        {
          "id": "ninebot.authorization",
          "name": "Authorization",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入，无需手填"
        },
        {
          "id": "ninebot.deviceId",
          "name": "DeviceId",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入，无需手填"
        },
        {
          "id": "ninebot.userAgent",
          "name": "User-Agent",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入，无需手填"
        },
        {
          "id": "ninebot.shareTaskUrl",
          "name": "分享动作接口 baseURL",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入（仅在出现分享任务时会捕获）"
        },
        {
          "id": "ninebot.progressStyle",
          "name": "盲盒进度条样式（0~7）",
          "val": "7",
          "type": "text",
          "desc": "可指定盲盒进度条样式。优先使用插件传入的 $argument.progressStyle"
        },
        {
          "id": "ninebot.titlePrefix",
          "name": "自定义通知标题",
          "val": "九号签到助手",
          "type": "text",
          "desc": "通知标题前缀，例如：九号签到助手"
        },
        {
          "id": "ninebot.logLevel",
          "name": "日志等级",
          "val": "info",
          "type": "select",
          "items": [
            { "key": "debug", "label": "调试（debug）" },
            { "key": "info", "label": "信息（info）" },
            { "key": "warn", "label": "警告（warn）" },
            { "key": "error", "label": "错误（error）" }
          ],
          "desc": "日志输出级别，默认 info"
        }
      ]
    }
  ]
}