{
  "id": "Ninebot.Sign.Single",
  "name": "九号智能电动车 · 单号自动签到（BoxJS）",
  "author": "QinyRui",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/logo_128.png",
  "repo": "https://t.me/JiuHaoAPP",
  "apps": [
    {
      "id": "ninebot.single",
      "name": "九号智能电动车（单号）",
      "keys": [
        "ninebot.authorization",
        "ninebot.deviceId",
        "ninebot.userAgent",
        "ninebot.shareTaskUrl",
        "ninebot.progressStyle",
        "ninebot.titlePrefix",
        "ninebot.logLevel"
      ],
      "settings": [
        {
          "id": "ninebot.authorization",
          "name": "Authorization",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入，无需手填"
        },
        {
          "id": "ninebot.deviceId",
          "name": "DeviceId",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入，无需手填"
        },
        {
          "id": "ninebot.userAgent",
          "name": "User-Agent",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入，无需手填"
        },
        {
          "id": "ninebot.shareTaskUrl",
          "name": "分享动作接口 baseURL",
          "val": "",
          "type": "text",
          "desc": "抓包自动写入（仅在出现分享任务时会捕获）"
        },
        {
          "id": "ninebot.progressStyle",
          "name": "盲盒进度条样式",
          "val": "7",
          "type": "select",
          "items": [
            { "key": "0", "label": "█░ 样式 0" },
            { "key": "1", "label": "▓░ 样式 1" },
            { "key": "2", "label": "▰▱ 样式 2" },
            { "key": "3", "label": "●○ 样式 3" },
            { "key": "4", "label": "■□ 样式 4" },
            { "key": "5", "label": "➤· 样式 5" },
            { "key": "6", "label": "▮▯ 样式 6" },
            { "key": "7", "label": "⣿⣀ 样式 7" }
          ],
          "desc": "盲盒进度条样式（图形展示），优先使用插件传入 $argument.progressStyle"
        },
        {
          "id": "ninebot.titlePrefix",
          "name": "自定义通知标题",
          "val": "九号签到助手",
          "type": "text",
          "desc": "通知标题前缀，例如：九号签到助手"
        },
        {
          "id": "ninebot.logLevel",
          "name": "日志等级",
          "val": "info",
          "type": "select",
          "items": [
            { "key": "debug", "label": "调试（debug）- 输出所有日志信息" },
            { "key": "info", "label": "信息（info）- 主要流程日志" },
            { "key": "warn", "label": "警告（warn）- 仅异常或网络错误" },
            { "key": "error", "label": "错误（error）- 仅错误信息" }
          ],
          "desc": "日志输出级别，可选择输出详细或简略日志"
        }
      ]
    }
  ]
}