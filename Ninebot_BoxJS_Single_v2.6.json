{
  "id": "ninebot.auto.sign",
  "name": "九号智能电动车自动签到（单账号）",
  "author": "QinyRui",
  "version": "2.6",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/icon/ninebot_128.png",
  "apps": [
    {
      "id": "Ninebot_Auto_Sign",
      "name": "九号签到 BoxJS 设置",
      "keys": [
        "ninebot.debugLevel",
        "ninebot.progressStyle",
        "ninebot.titlePrefix",
        "ninebot.autoOpenBox"
      ],
      "settings": [
        {
          "id": "ninebot.debugLevel",
          "name": "调试日志等级",
          "type": "select",
          "val": "INFO",
          "list": ["OFF", "WARN", "ERROR", "INFO", "DEBUG", "ALL"]
        },
        {
          "id": "ninebot.progressStyle",
          "name": "盲盒进度条样式",
          "type": "select",
          "val": "0",
          "list": ["0","1","2","3","4","5","6","7"]
        },
        {
          "id": "ninebot.titlePrefix",
          "name": "自定义通知标题",
          "type": "text",
          "val": "九号签到"
        },
        {
          "id": "ninebot.autoOpenBox",
          "name": "自动开启7天盲盒",
          "type": "boolean",
          "val": true
        }
      ]
    }
  ]
}