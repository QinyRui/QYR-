{
  "id": "ninebot.auto.sign",
  "name": "九号智能电动车自动签到（单账号）",
  "author": "QinyRui",
  "version": "2.6",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/icon/ninebot_128.png",
  "apps": [
    {
      "id": "Ninebot_Auto_Sign",
      "name": "九号签到",
      "keys": [
        "ninebot.authorization",
        "ninebot.deviceId",
        "ninebot.userAgent",
        "ninebot.debug",
        "ninebot.notify",
        "ninebot.autoOpenBox",
        "ninebot.autoRepair",
        "ninebot.notifyFail",
        "ninebot.titlePrefix",
        "ninebot.shareTaskUrl",
        "ninebot.progressStyle"
      ],
      "settings": [
        {"id":"ninebot.authorization","name":"Authorization","type":"text","val":""},
        {"id":"ninebot.deviceId","name":"DeviceId","type":"text","val":""},
        {"id":"ninebot.userAgent","name":"User-Agent（自动写入）","type":"text","val":""},
        {"id":"ninebot.debug","name":"调试日志","type":"boolean","val":true},
        {"id":"ninebot.notify","name":"通知开关","type":"boolean","val":true},
        {"id":"ninebot.autoOpenBox","name":"自动开启盲盒","type":"boolean","val":true},
        {"id":"ninebot.autoRepair","name":"自动补签","type":"boolean","val":true},
        {"id":"ninebot.notifyFail","name":"签到失败也通知","type":"boolean","val":true},
        {"id":"ninebot.titlePrefix","name":"自定义通知标题","type":"text","val":"九号签到"},
        {"id":"ninebot.shareTaskUrl","name":"分享任务接口 URL（自动写入）","type":"text","val":""},
        {
          "id":"ninebot.progressStyle",
          "name":"盲盒进度条样式",
          "type":"select",
          "val":0,
          "options":[
            {"val":0,"name":"█░ (默认)"},
            {"val":1,"name":"▓░"},
            {"val":2,"name":"▰▱"},
            {"val":3,"name":"●○"},
            {"val":4,"name":"■□"},
            {"val":5,"name":"➤·"},
            {"val":6,"name":"▮▯"},
            {"val":7,"name":"⣿⣀"},
            {"val":8,"name":"1 天：[⣿░]"},
            {"val":9,"name":"2 天：[⣿⣿░]"},
            {"val":10,"name":"3 天：[⣿⣿⣿░]"},
            {"val":11,"name":"4 天：[⣿⣿⣿⣿░]"},
            {"val":12,"name":"5 天：[⣿⣿⣿⣿⣿░]"},
            {"val":13,"name":"6 天：[⣿⣿⣿⣿⣿⣿░]"},
            {"val":14,"name":"7 天：[⣿⣿⣿⣿⣿⣿⣿░]"}
          ]
        }
      ]
    }
  ]
}