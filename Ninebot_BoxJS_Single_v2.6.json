{
  "id": "ninebot.auto.sign",
  "name": "九号智能电动车自动签到（单账号）",
  "author": "QinyRui",
  "version": "2.6",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/icon/ninebot_128.png",
  "apps": [
    {
      "id": "Ninebot_Auto_Sign",
      "name": "九号签到 BoxJS 设置",
      "keys": [
        "ninebot.debugLevel",
        "ninebot.progressStyle",
        "ninebot.titlePrefix",
        "ninebot.autoOpenBox"
      ],
      "settings": [
        {
          "id": "ninebot.debugLevel",
          "name": "调试日志等级（0=OFF,1=WARN,2=ERROR,3=INFO,4=DEBUG,5=ALL）",
          "type": "number",
          "val": 3
        },
        {
          "id": "ninebot.progressStyle",
          "name": "盲盒进度条样式（0-7）",
          "type": "number",
          "val": 0
        },
        {
          "id": "ninebot.titlePrefix",
          "name": "自定义通知标题",
          "type": "text",
          "val": "九号签到"
        },
        {
          "id": "ninebot.autoOpenBox",
          "name": "自动开启7天盲盒",
          "type": "boolean",
          "val": true
        }
      ]
    }
  ]
}