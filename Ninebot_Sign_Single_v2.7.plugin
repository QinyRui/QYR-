[Argument]
logLevel = select,"OFF","WARN","ERROR","INFO","DEBUG","ALL",tag=日志等级,desc=选择控制台日志输出等级
style = select,"0","1","2","3","4","5","6","7",tag=盲盒样式,desc=选择盲盒进度条样式

[Script]
cron 0 12 * * * script-path=https://raw.githubusercontent.com/YourRepo/Ninebot_Sign_Single_v2.7.plugin, timeout=10, tag=九号-参数测试