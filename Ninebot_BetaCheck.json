    {"key": "ninebot.notify", "name": "推送通知", "type": "boolean", "default": true},
    {"key": "ninebot.debug", "name": "调试日志", "type": "boolean", "default": false},
    {"key": "ninebot.titlePrefix", "name": "通知标题前缀", "type": "text", "default": "九号内测"}
  ],
  "scripts": [
    {
      "name": "Ninebot_BetaCheck",
      "path": "https://raw.githubusercontent.com/你的仓库/Ninebot_BetaCheck.js",
      "type": "cron",
      "interval": 1440,
      "tag": "九号内测检测"
    }
  ]
