{
  "version": "1.0",
  "name": "九号内测资格检测",
  "author": "QinyRui",
  "description": "检测九号电动车内测资格，自动申请内测，支持重试和通知",
  "configs": [
    {"key": "ninebot.authorization", "name": "Authorization", "type": "text"},
    {"key": "ninebot.deviceId", "name": "DeviceId", "type": "text"},
