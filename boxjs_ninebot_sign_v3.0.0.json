{
  "id": "Ninebot.Sign.Single",
  "name": "九号智能电动车 · 单号自动签到",
  "author": "QinyRui",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/logo_128.png",
  "repo": "https://t.me/JiuHaoAPP",
  "apps": [
    {
      "id": "ninebot.single",
      "name": "九号智能电动车（单号）",
      "keys": [
        "#ComponentService",
        "ninebot.titlePrefix",
        "ninebot.notify",
        "ninebot.autoOpenBox",
        "ninebot.notifyFail",
        "ninebot.logLevel",
        "ninebot.captureEnable",
        "ninebot.lastCaptureAt"
      ],
      "settings": [
        {
          "id": "ninebot.captureEnable",
          "name": "自动抓包开关",
          "val": "true",
          "type": "select",
          "items": [
            {"key": "true", "label": "开启（自动抓包写入）"},
            {"key": "false", "label": "关闭（仅手动填写）"}
          ],
          "desc": "开启后访问九号签到页自动抓取凭证，无需手动触发抓包"
        },
        {
          "id": "ninebot.titlePrefix",
          "name": "自定义通知标题",
          "val": "九号签到助手",
          "type": "text",
          "desc": "通知标题前缀，如“九号签到助手”"
        },
        {
          "id": "ninebot.notify",
          "name": "开启签到通知",
          "val": "true",
          "type": "select",
          "items": [
            {"key": "true", "label": "开启（推送结果）"},
            {"key": "false", "label": "关闭（不推送）"}
          ],
          "desc": "是否推送签到结果通知"
        },
        {
          "id": "ninebot.autoOpenBox",
          "name": "自动开启盲盒",
          "val": "true",
          "type": "select",
          "items": [
            {"key": "true", "label": "开启（自动开箱）"},
            {"key": "false", "label": "关闭（手动开箱）"}
          ],
          "desc": "支持所有可开启盲盒自动领取奖励"
        },
        {
          "id": "ninebot.notifyFail",
          "name": "失败通知开关",
          "val": "true",
          "type": "select",
          "items": [
            {"key": "true", "label": "开启（失败提醒）"},
            {"key": "false", "label": "关闭（不提醒）"}
          ],
          "desc": "仅在任务失败时推送通知"
        },
        {
          "id": "ninebot.logLevel",
          "name": "日志输出级别",
          "val": "simple",
          "type": "select",
          "items": [
            {"key": "silent", "label": "静默（无日志）"},
            {"key": "simple", "label": "简洁（仅错误）"},
            {"key": "full", "label": "完整（所有日志）"}
          ],
          "desc": "控制日志详细程度"
        },
        {
          "id": "ninebot.lastCaptureAt",
          "name": "最后抓包时间",
          "val": "",
          "type": "text",
          "desc": "凭证最后更新时间（仅展示，自动同步）"
        }
      ]
    }
  ]
}