{
  "id": "ninebot.single.sign",
  "name": "九号智能电动车 · 单号自动签到（含分享奖励）",
  "author": "@QinyRui",
  "icon": "https://raw.githubusercontent.com/QinyRui/QYR-/jiuhao/icon.png",
  "repo": "https://t.me/JiuHaoAPP",
  "apps": [
    {
      "id": "ninebot.sign.config",
      "name": "九号签到助手 - 配置",
      "keys": [
        "Ninebot_Authorization",
        "Ninebot_DeviceId",
        "Ninebot_UA",
        "Ninebot_notify",
        "Ninebot_capture",
        "Ninebot_autoRepair",
        "Ninebot_titlePrefix",
        "Ninebot_cron_time",
        "Ninebot_logLevel"
      ],
      "settings": [
        {
          "id": "Ninebot_notify",
          "name": "通知开关",
          "val": "true",
          "type": "boolean"
        },
        {
          "id": "Ninebot_capture",
          "name": "抓包写入开关",
          "val": "false",
          "type": "boolean"
        },
        {
          "id": "Ninebot_autoRepair",
          "name": "自动修复分享任务",
          "val": "true",
          "type": "boolean"
        },
        {
          "id": "Ninebot_titlePrefix",
          "name": "自定义通知标题",
          "val": "九号签到助手",
          "type": "text"
        },
        {
          "id": "Ninebot_cron_time",
          "name": "签到时间（CRON）",
          "val": "1 0 * * *",
          "type": "text"
        },
        {
          "id": "Ninebot_logLevel",
          "name": "脚本日志等级",
          "val": "info",
          "type": "text",
          "desc": "debug=最详细，error=仅报错"
        }
      ]
    }
  ]
}