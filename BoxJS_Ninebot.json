{
  "title": "九号智能电动车自动签到",
  "version": "v2.2 FullMulti",
  "author": "❥﹒﹏非我不可",
  "description": "多账号管理，支持签到、盲盒领取、N币查询，日志/通知控制",
  "settings": [
    {
      "type": "toggle",
      "key": "debug",
      "default": true,
      "title": "调试日志",
      "desc": "开启后在控制台显示详细日志"
    },
    {
      "type": "toggle",
      "key": "notify",
      "default": true,
      "title": "通知开关",
      "desc": "开启后会发送签到通知"
    },
    {
      "type": "input",
      "key": "titlePrefix",
      "default": "九号签到",
      "title": "通知标题前缀"
    },
    {
      "type": "toggle",
      "key": "autoOpenBox",
      "default": true,
      "title": "自动开启盲盒",
      "desc": "开启后，满足条件的盲盒会自动领取奖励"
    },
    {
      "type": "input",
      "key": "concurrentDelayMs",
      "default": "600",
      "title": "账号间延迟(ms)",
      "desc": "多账号循环时，每个账号执行完延迟时间，防止接口异常"
    },
    {
      "type": "list",
      "key": "Ninebot_Accounts",
      "title": "账号列表",
      "desc": "可添加多个账号，每个账号可单独填写 Authorization 与 DeviceId",
      "items": [
        {
          "type": "input",
          "key": "name",
          "title": "账号名称"
        },
        {
          "type": "input",
          "key": "Authorization",
          "title": "Authorization Token"
        },
        {
          "type": "input",
          "key": "DeviceId",
          "title": "DeviceId"
        }
      ]
    },
    {
      "type": "button",
      "key": "TestNotify",
      "title": "测试通知",
      "desc": "点击发送测试通知"
    },
    {
      "type": "button",
      "key": "TestLog",
      "title": "测试日志",
      "desc": "点击输出日志测试"
    }
  ]
}